generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id          Int     @id @default(autoincrement())
  stockCode   String  @unique
  description String
  price       Float
  stockLevel  Int
  // Add more fields as needed, e.g., createdAt DateTime @default(now())
}
