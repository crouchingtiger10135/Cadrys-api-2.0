generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id          Int      @id @default(autoincrement())
  stockCode   String   @unique
  description String?
  price       Decimal
  stockLevel  Int
  createdAt   DateTime @default(now())
  length      Float?
  name        String?
  origin      String?
  size        String?
  sku         String?
  width       Float?
}
